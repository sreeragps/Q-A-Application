@page
@{
    Layout = "_StackOverFlowLayout";
}
@model ListModel

<h1>
    <BOLD>... QUESTION LIST.....QUESTION LIST.....QUESTION LIST...</BOLD>
</h1>

<body>

    @for (int i = 0; i < Model.QuestionList1.Count; i++)
    {

        <label>
            <strong>QUESTION</strong> :@Model.QuestionList1[i].StackQuestion
        </label><br>

        <label><strong>ANSWER:</strong></label><br>
        <p style="border:3px; border-style:solid; border-color:#FF0000; padding: 1em;">@Model.QuestionList1[i].StackAnswer.</p><br>
    }

</body>